<resources>
    <declare-styleable name="PullLayout">
        <attr name="pull_position" format="enum">
            <enum name="left" value="0" />
            <enum name="top" value="1" />
            <enum name="right" value="2" />
            <enum name="bottom" value="3" />
        </attr>
        <attr name="pull_overlay" format="boolean" />
        <attr name="pull_threshold" format="dimension" />
    </declare-styleable>
    <declare-styleable name="SingleLoadingStatusView">
        <attr name="custom_layout" format="reference" />
    </declare-styleable>
</resources>
